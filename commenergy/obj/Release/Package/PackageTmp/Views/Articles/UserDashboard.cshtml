@model commenergy.Models.Article

@{
    ViewBag.Title = "UserDashboard";
}

<h2>UserDashboard</h2>
<div id="d">
    <div class="row" data-bind="foreach: Article">
    <div style="width: 25%; height: auto;  text-align:center;padding: 22px 10px 0px 0px">
        <p data-bind="text: Author"></p>
        <h2 data-bind="text: Title"></h2>
        <p data-bind="text: Body"></p>
    </div>
</div>
    </div>


@section Scripts {
    <script src="~/Scripts/knockout-3.0.0.js"></script>
    <script src="~/Scripts/ViewModels/user.js"></script>
}